<%= render 'shared/admin-navbar' %>

<div class="container">


    <h3>Recipe lists:</h3>
    <h5><%= link_to 'Nouvelle liste', new_recipe_list_path, class:"btn btn-outline-primary" %></h5>

    <% unless @recipe_lists.nil? %>
        <ol>
          <% @recipe_lists.order(:id).reverse.each do |list| %>
            <li>
              (<%= list.id %>)
              #<%= list.tag_list %>
              <%= link_to list.name, recipe_list_path(list) %>
            </li>
          <% end %>
        </ol>
    <% end %>


</div>
