<div class="jumbotron shadow-lg collapse py-1 pl-3 pr-1 d-flex flex-column">
  <i class="fas fa-2x fa-times close text-right m-3" style="cursor: pointer;" id="closeRatingProgress"></i>

    <div>
    <% list.tasks.each do |task| %>
      <div class="mb-3">
        <p class="h2"><%= task.name %></p>
        <%= render "lists/task_progress", task_item: TaskItem.find_by(list: list, task: task) %>
      </div>
    <% end %>
  </div>
</div>

