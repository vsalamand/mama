  <% if list.sorted_by == "category" %>
    <%= render 'lists/category', list: list %>


  <% elsif list.sorted_by == "rayon" %>
    <%= render 'lists/store_section', list: list %>

  <% elsif list.sorted_by == "foodgroup" %>
    <%= render 'lists/foodgroup', list: list %>


  <% else list.sorted_by == "date" || nil %>
      <div id="date">
        <% list.items.not_deleted.sort_by(&:id).each do |item| %>
          <%= render 'items/show', item: item %>
        <% end %>
      </div>

  <% end %>
